{
  "name": "@vessel/integration-sdk",
  "version": "1.0.0",
  "description": "SDK para integrar Vessel en aplicaciones externas",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "vessel",
    "inventory",
    "sdk",
    "integration"
  ],
  "author": "Vessel Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
